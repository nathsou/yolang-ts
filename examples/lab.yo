import std/ops
import std/strings
import std/arrays

type S = {
  a: i32,
  b: bool,
  c: i32[],
}

fun [](a: i32[], index: u32): i32 {
  a.data.getUnchecked(index)
}

fun []=(mut a: i32[], index: u32, value: i32): void {
  a.data.setUnchecked(index, value)
}

fun main() {
  mut s = S { a: 11, b: true, c: [1, 2, 3] }

  log(s.a)
  log(s.b)
  log(s.c[0u32])

  0 // unix exit code
}
